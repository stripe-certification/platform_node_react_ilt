{
  "name": "stripe-platform-ilt",
  "version": "0.1.0",
  "description": "Stripe Platform Ilt",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "npx tsx src/server.ts",
    "startServerDev": "tsx watch src/server.ts",
    "startListener": "stripe listen --forward-to http://localhost:4242/webhook",
    "dev": "concurrently -i --kill-others \"npm run startServerDev\" \"npm run startListener\"",
    "lint": "npx eslint",
    "prettier": "prettier './**/*.{js,ts,md}' --write",
    "prettier-check": "prettier './**/*.{js,ts,md}' --check"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/cors": "^2.8.17",
    "@types/express-session": "^1.18.1",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "stripe": "^17.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.10",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.13.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-spellcheck": "^0.0.20",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  }
}
